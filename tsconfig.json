{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "esModuleInterop": true,
    "sourceMap": true,
    "declaration": false,
    "module": "preserve",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "ES2020",
    "typeRoots": ["node_modules/@types"],
    "lib": ["ES2020", "dom", "ES2021.String"],
    "skipLibCheck": true
  },
  "angularCompilerOptions": {
    "strictTemplates": true
  },
  "snippets": {
    "skipLibCheck": true
  }
}
