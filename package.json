  "scripts": {
    "build": "node ./utils/prebuild.js & ng build --configuration=production --stats-json --aot=true",
    "builddev": "node ./utils/prebuild.js & ng build --configuration=production --stats-json --aot=true --output-path=dist/ --base-href http://localhost:3100",
    "buildprod": "node ./utils/prebuild.js & ng build --configuration=production --stats-json --aot=true --output-path=dist/",
    "build:jit": "ng build -prod --aot=false",
    "dbdump": "mongodump --archive=../data/dbbackup/kia.dev.archive --db kompendia",
    "dbrestore": "mongorestore --drop --archive=../data/dbbackup/kompendia-2022-03-07_03_00.archive --db kompendia",
    "oid": "node ./utils/objectid.js",
    "serve": "node ./utils/prebuild.js & ng serve --proxy-config proxy.conf.json",
    "serve:wlan": "node ./utils/prebuild.js & ng serve --proxy-config proxy.conf.json --host 192.168.178.30",
    "test": "ng test --include='**/admin/*.spec.ts'",
    "lint": "ng lint",
    "lintfix": "ng lint --fix",
    "lints": "ng lint --files src/app/engine/prim-main.ts --files src/app/engine/prim-worker.ts",
    "e2e": "ng e2e",
    "outdated": "npm outdated",
    "update": "npm update",
    "dev:ssr": "ng run kompendia:serve-ssr",
    "serve:ssr": "node dist/kompendia/server/main.js",
    "build:ssr": "node ./utils/prebuild.js & ng build --configuration production --stats-json && ng run kompendia:server:production",
    "analyzer": "webpack-bundle-analyzer dist/kompendia/browser/stats.json",
    "analyzer:ssr": "webpack-bundle-analyzer dist/kompendia/browser/stats.json",
    "prerender": "ng run kompendia:prerender --routes-file ../data/sitemaps/forming/routes.txt",
    "service": "ng run kompendia:service-work",
    "prebuild:ssr": "ngcc",
    "scully": "npx scully -- --project kompendia",
    "scully:prod": "npx scully -- --configFile scully.kompendia-prod.config.ts",
    "scully:dev": "npx scully -- --configFile scully.kompendia-dev.config.ts",
    "scully:serve": "npx scully serve -- --project kompendia"
  },
  "private": true,
